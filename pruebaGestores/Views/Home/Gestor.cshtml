@{
    ViewBag.Title = "Gestor";
}
<h3>Gestores</h3>
<div class="card-body">

    <div class="col-sm-12" style="max-height: 70vh; overflow-y: auto;">
        <table id="dgvUsuarios" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Gestor</th>
                </tr>
            </thead>
            <tbody>
                @foreach (pruebaGestores.Models.ModelGestor _model in Model)
                {
                    <tr>
                        <td>@_model.IdGestor</td>
                        <td>@_model.Gestor</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>